{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "server.js"
    }
  ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://tomato-food-del-frontend.vercel.app" 
      ]
    }
    ]
 
}
